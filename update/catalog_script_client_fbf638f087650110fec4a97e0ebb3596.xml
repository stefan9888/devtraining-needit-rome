	getUserEmail.addParam('sysparm_name', 'GetEmail');
	getUserEmail.addParam('sysparm_ceVreauEu', g_form.getValue('requestor'));
	getUserEmail.getXML(populateEmailField);
	//when response is back from the server
	function populateEmailField(response){
		var emailFromScriptInclude = response.responseXML.documentElement.getAttribute("answer");
		g_form.setValue('email', emailFromScriptInclude);
